<div class = "container">
   <%= form_with url:create_avatar_path(:project_id =>@project.id), html: { multipart: true }, action: "post", local: true do |f| %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
</div>